<!DOCTYPE html>
<html ng-app="nodeBotsApp">
  <head>
    <meta charset="utf-8">
    <title>Control Remoto | NodeBots</title>
    <link rel="stylesheet" href="bower/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="bower/bootstrap/dist/css/bootstrap-theme.min.css">
    <link href='http://fonts.googleapis.com/css?family=Monda:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="css/main.css">
  </head>
  <body ng-controller="appCtrl">

    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">

          <div class="masthead clearfix">
            <div class="inner">
              <h3 class="masthead-brand">NodeBots El Salvador</h3>
              <nav>
                <ul class="nav masthead-nav">
                  <li class=""><a href="/">Inicio</a></li>
                  <li class="active"><a href="#">App</a></li>
                </ul>
              </nav>
            </div>
          </div>

          <div class="inner cover">
            <h1 class="cover-heading">Control Remoto</h1>
          </div>

          <div class="mastfoot">
            <div class="inner">
              <p>Cover template for <a href="http://getbootstrap.com">Bootstrap</a></p>
            </div>
          </div>

          <div class="row" id="btns">
            <div class="col-md-12">
              <button type="button" class="btn btn-default" id="up" ng-click="up()">
                <span class="glyphicon glyphicon-chevron-up" aria-hidden="true"></span>
              </button>
            </div>
            <div class="col-md-12">
              <button type="button" class="btn btn-danger" id="stop" ng-click="stop()">
                <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
              </button>
            </div>
            <div class="col-md-12">
              <button type="button" class="btn btn-default" id="down" ng-click="down()">
                <span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span>
              </button>
            </div>
          </div>

        </div>

      </div>
    </div>




    <script src="bower/jquery/dist/jquery.min.js"></script>
    <script src="bower/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="bower/angular/angular.min.js"></script>
    <script src="js/app.js"></script>
  </body>
</html>
